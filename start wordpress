root@kali:~/Downloads/wordpress-4.9.4/wordpress# Ls wordpress/
bash: Ls: command not found
root@kali:~/Downloads/wordpress-4.9.4/wordpress# service apache2 start
root@kali:~/Downloads/wordpress-4.9.4/wordpress# service mariadb start
